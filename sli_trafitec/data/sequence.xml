<?xml version="1.0" encoding="utf-8"?>

<odoo>

    <record id="sli_trafitec_cotizacion_secuencia" model="ir.sequence">
        <field name="name">sli_trafitec_cotizacion_secuencia</field>
        <field name="code">trafitec.cotizacion</field>
        <field name="prefix">CO</field>
        <field name="padding">5</field>
    </record>

    <record id="sequences_projects" model="ir.sequence">
        <field name="name">Folio Proyecto</field>
        <field name="code">trafitec.cotizacion</field>
        <field name="active">TRUE</field>
        <field name="prefix"></field>
        <field name="padding">10</field>
        <field name="number_increment">1</field>
    </record>

    <record id="sli_trafitec_cotizaciones_origen_destino_secuencia" model="ir.sequence">
        <field name="name">sli_trafitec_cotizaciones_origen_destino_secuencia</field>
        <field name="code">trafitec.cotizaciones.linea.origen</field>
        <field name="padding">0</field>
    </record>

    <record id="sli_trafitec_viajes_secuencia" model="ir.sequence">
        <field name="name">sli_trafitec_viajes_secuencia</field>
        <field name="code">trafitec.viajes</field>
        <field name="prefix">OC</field>
        <field name="padding">5</field>
    </record>

    <record id="sli_trafitec_contrarecibos_secuencia" model="ir.sequence">
        <field name="name">sli_trafitec_contrarecibos_secuencia</field>
        <field name="code">trafitec.contrarecibo</field>
        <field name="prefix">CR</field>
        <field name="padding">5</field>
    </record>

    <record id="sequences_travels" model="ir.sequence">
        <field name="name">Folio Viajes</field>
        <field name="code">trafitec.viajes</field>
        <field name="active">TRUE</field>
        <field name="prefix"></field>
        <field name="padding">10</field>
        <field name="number_increment">1</field>
    </record>

</odoo>