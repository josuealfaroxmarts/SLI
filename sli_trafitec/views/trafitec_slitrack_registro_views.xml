<?xml version="1.0" encoding="UTF-8"?>

<odoo>
    <data>
        
        <record id="trafitec_slitrack_registro_form_view" model="ir.ui.view">
            <field name="name">trafiteck.slitrack.registro.form</field>
            <field name="model">trafitec.slitrack.registro</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="viaje_id" readonly="1" options="{'no_open': True}"/>
                        <field name="fechahorag" string="Fecha y hora de registro" />
                        <field name="latitud" />
                        <field name="longitud" />/>
                        <field name="velocidad" />/>
                        <field name="proveedor" />/>
                    </group>
                </form>
            </field>
        </record>

    </data>
</odoo>