<?xml version="1.0" encoding="utf-8"?>

<odoo>
	<data>

		<record id="fee_report_tree_view" model="ir.ui.view">
            <field name="name">Fee Report</field>
            <field name="model">fee.report</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="id"/>
                    <field name="partner_id"/>
                    <field name="state"/>
                    <field name="move_name"/>
                </tree>
            </field>
        </record>

		<record id="fee_report_action_window" model="ir.actions.act_window">
			<field name="name">Fee Report</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">fee.report</field>
			<field name="view_mode">tree</field>
			<field name="auto_search">true</field>
			<field name="limit">10</field>
		</record>

	</data>
</odoo>