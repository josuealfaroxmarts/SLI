<openerp>
<data>


<!-- Inheritance for ir.sequence to assign a Sequence for each Shop -->
    <record model="ir.ui.view" id="fleet_mro_view_operating_unit_form">
        <field name="name">MRO operating.unit.form</field>
        <field name="model">operating.unit</field>
        <field name="type">form</field>
        <field name="priority">50</field>
        <field name="inherit_id" ref="operating_unit.view_operating_unit_form"/>
        <field name="arch" type="xml">
            <xpath expr="/form" position="inside">
                <group colspan="4" col="4">
                    <separator string="MRO Sequences" colspan="4" />
                    <field name="fleet_mro_service_order_seq"/>
                    <newline />
                    <field name="fleet_mro_driver_report_seq" />
                </group>
            </xpath>
        </field>
    </record>

</data>
</openerp>
