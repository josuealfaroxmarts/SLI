<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record model="ir.ui.view" id="fleet_mro_hr_employee_mro_productive_tree">
            <field name="name">fleet.mro.hr.employee.mro_productive.tree</field>
            <field name="model">hr.employee.mro_productive</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Task Event">
                    <field name="productivity_type" domain="[('type','=','not_busy')]"/>
                    <field name="type" />
                    <field name="date_start" />
                    <field name="date_end" />
                    <field name="duration" widget="float_time"/>
                    <field name="complete"/>
                </tree>
            </field>
        </record>
    </data>
</openerp>
